        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="dataset reader" enabled="true">
          <stringProp name="BeanShellSampler.query">import java.io.File;
import java.nio.file.Files;
import java.nio.file.Paths;

String folderPath = &quot;datasets/&quot;;

File folder = new File(folderPath);
File[] files = folder.listFiles();

vars.put(&quot;numOfDocs&quot;, String.valueOf(files.length));
log.info(&quot;numOfDocs=&quot; + String.valueOf(files.length));

for (int i = 0; i &lt; files.length; i++) {
	log.info(&quot;Reading file &quot; + files[i].getPath());
	String fileContent = &quot;&quot;;
    try {
        fileContent = new String(Files.readAllBytes(Paths.get(files[i].getPath(), new String[0])));
    } catch (Exception e) {
        log.error(&quot;Error reading file: &quot; + files[i].getName(), e);
    }

    String base64String = Base64.getEncoder().encodeToString(fileContent.getBytes(&quot;UTF-8&quot;));

    vars.put(&quot;fileContentsArray_&quot; + i, base64String);
}</stringProp>
          <boolProp name="BeanShellSampler.resetInterpreter">false</boolProp>
        </BeanShellSampler>
